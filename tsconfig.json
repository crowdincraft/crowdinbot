{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "lib": [
      "es2020"
    ],
    "module": "commonjs",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "outDir": "bin",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
